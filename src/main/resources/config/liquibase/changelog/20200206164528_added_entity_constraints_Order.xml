<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Order.
    -->
    <changeSet id="20200206164528-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="client_id"
                                 baseTableName="jhi_order"
                                 constraintName="fk_jhi_order_client_id"
                                 referencedColumnNames="id"
                                 referencedTableName="client"/>

        <addForeignKeyConstraint baseColumnNames="order_id"
                                 baseTableName="jhi_order_products"
                                 constraintName="fk_jhi_order_products_jhi_order_id"
                                 referencedColumnNames="id"
                                 referencedTableName="jhi_order"/>

        <addForeignKeyConstraint baseColumnNames="products_id"
                                 baseTableName="jhi_order_products"
                                 constraintName="fk_jhi_order_products_products_id"
                                 referencedColumnNames="id"
                                 referencedTableName="product"/>
        
    </changeSet>
</databaseChangeLog>
